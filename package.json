{
  "name": "freelance-hub",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": {
    "packages": [
      "./apps/*",
      "./client"
    ]
  },
  "scripts": {
    "dev:build": "docker-compose up --build",
    "dev": "docker-compose up",
    "down": "docker-compose down",
    "build:server": "nest build",
    "build:client": "npm run build -C ./client",
    "build": "npm run build:server && npm run build:client",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\" \"client/**/*.{ts,tsx}\" --config ./.prettierrc",
    "start:dev": "nest start --tsc --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"apps/**/*.ts\" \"libs/**/*.ts\" \"client/**/*.{ts,tsx}\" --fix"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "^6.17.0",
    "@types/otplib": "^7.0.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.11",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "streamifier": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-google-oauth": "^1.0.45",
    "@types/passport-google-oauth2": "^0.1.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.5",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "prisma": {
    "schema": "apps/users-service/prisma/schema.prisma"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
